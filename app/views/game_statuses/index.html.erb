<p id="notice"><%= notice %></p>

<h1>Listing Game Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Gamestatusid</th>
      <th>Gameconfigid</th>
      <th>Gamelevel</th>
      <th>Snapshotid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @game_statuses.each do |game_status| %>
      <tr>
        <td><%= game_status.gameStatusID %></td>
        <td><%= game_status.gameConfigID %></td>
        <td><%= game_status.gameLevel %></td>
        <td><%= game_status.snapshotID %></td>
        <td><%= link_to 'Show', game_status %></td>
        <td><%= link_to 'Edit', edit_game_status_path(game_status) %></td>
        <td><%= link_to 'Destroy', game_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Game status', new_game_status_path %>
