<p id="notice"><%= notice %></p>

<h1>Listing Players</h1>

<table>
  <thead>
    <tr>
      <th>Playerid</th>
      <th>Sessionid</th>
      <th>Nickname</th>
      <th>Password</th>
      <th>Role</th>
      <th>Priviledges</th>
      <th>Firstname</th>
      <th>Surname</th>
      <th>Lastlogintime</th>
      <th>Gamesubsriptionid</th>
      <th>Gametologid</th>
      <th>Gameeventlogid</th>
      <th>Loggingonofftoggle</th>
      <th>Elogperiodbeging</th>
      <th>Elogperiodend</th>
      <th>Eventlogginginfo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @players.each do |player| %>
      <tr>
        <td><%= player.playerID %></td>
        <td><%= player.sessionID %></td>
        <td><%= player.nickName %></td>
        <td><%= player.password %></td>
        <td><%= player.role %></td>
        <td><%= player.priviLedges %></td>
        <td><%= player.firstName %></td>
        <td><%= player.surName %></td>
        <td><%= player.lastLogInTime %></td>
        <td><%= player.gameSubsriptionID %></td>
        <td><%= player.gameToLogID %></td>
        <td><%= player.gameEventLogID %></td>
        <td><%= player.loggingOnOffToggle %></td>
        <td><%= player.eLogPeriodBeging %></td>
        <td><%= player.eLogPeriodEnd %></td>
        <td><%= player.eventLoggingInfo %></td>
        <td><%= link_to 'Show', player %></td>
        <td><%= link_to 'Edit', edit_player_path(player) %></td>
        <td><%= link_to 'Destroy', player, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Player', new_player_path %>
