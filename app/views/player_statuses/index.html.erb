<p id="notice"><%= notice %></p>

<h1>Listing Player Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Playerid</th>
      <th>Timestamp</th>
      <th>Gamestatus</th>
      <th>Gameconfigid</th>
      <th>Gamelevel</th>
      <th>Gamescore1</th>
      <th>Gamescore2</th>
      <th>Gamescore3</th>
      <th>Gamescore4</th>
      <th>Gamescore5</th>
      <th>Gamescore6</th>
      <th>Gamescore7</th>
      <th>Gamescore8</th>
      <th>Gamescore9</th>
      <th>Gamescore10</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @player_statuses.each do |player_status| %>
      <tr>
        <td><%= player_status.playerID %></td>
        <td><%= player_status.timeStamp %></td>
        <td><%= player_status.gameStatus %></td>
        <td><%= player_status.gameConfigID %></td>
        <td><%= player_status.gameLevel %></td>
        <td><%= player_status.gameScore1 %></td>
        <td><%= player_status.gameScore2 %></td>
        <td><%= player_status.gameScore3 %></td>
        <td><%= player_status.gameScore4 %></td>
        <td><%= player_status.gameScore5 %></td>
        <td><%= player_status.gameScore6 %></td>
        <td><%= player_status.gameScore7 %></td>
        <td><%= player_status.gameScore8 %></td>
        <td><%= player_status.gameScore9 %></td>
        <td><%= player_status.gameScore10 %></td>
        <td><%= link_to 'Show', player_status %></td>
        <td><%= link_to 'Edit', edit_player_status_path(player_status) %></td>
        <td><%= link_to 'Destroy', player_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Player status', new_player_status_path %>
